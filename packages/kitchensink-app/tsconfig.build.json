{
  "extends": "../../tsconfig.build.json",
  "compilerOptions": {
    "lib": ["ESNext"],
    "allowJs": true,
    "checkJs": true,
    "jsx": "react-native",
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": false,
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
